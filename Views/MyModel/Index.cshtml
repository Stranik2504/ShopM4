@model IEnumerable<MyModel>
@{
    ViewData["Title"] = "MyModels";
}

@if (Model.Count() > 0)
{
    <table class="table table-bordered" style="width:100%">
        <thead>
            <tr>
                <th>Name</th>
                <th>Number</th>
                <th></th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td width="50%">@item.Name</td>
                    <td width="30%">@item.Number</td>
                    <td width="20%"></td>
                </tr>
            }
        </tbody>
    </table>
}
